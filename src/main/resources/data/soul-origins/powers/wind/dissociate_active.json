{
    "type": "origins:multiple",
    "slowfall": {
        "type": "origins:modify_falling",
        "take_fall_damage": true,
        "__comment": "      ^ If fall damage is false then it doesn't trigger action_on_land",
        "velocity": 0.005
    },
    "airspeed": {
        "type": "origins:modify_air_speed",
        "modifier": {
            "operation": "multiply_base",
            "value": 4
        }
    },
    "invisible": {
        "type": "origins:invisibility",
        "render_armor": false,
        "render_outline": false
    },
    "noaggro": {
        "type": "apugli:mobs_ignore"
    },
    "nodamage": {
        "type": "origins:modify_damage_taken",
        "damage_condition": {
            "type": "origins:in_tag",
            "tag": "minecraft:is_fall"
        },
        "modifier": {
            "operation": "set_total",
            "value": 0
        }
    },
    "shader": {
        "type": "origins:shader",
        "shader": "minecraft:shaders/post/desaturate.json"
    },
    "remove": {
        "type": "origins:action_on_land",
        "entity_action": {
            "type": "origins:delay",
            "ticks": 1,
            "action": {
                "type": "origins:revoke_power",
                "power": "soul-origins:wind/dissociate_active",
                "source": "soul-origins:toggle"
            }
        }
    }
}
